<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net35</TargetFramework>
    <AssemblyName>JetBrains.Rider.Unity.Editor.Plugin.Full</AssemblyName>
    <RootNamespace>JetBrains.Rider.Unity.Editor</RootNamespace>
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <DefineConstants>UNITY_2017_3;RIDER_EDITOR_PLUGIN</DefineConstants>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <AssemblyOriginatorKeyFile>..\..\sign.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    
    <PackageReference Include="JetBrains.RdFramework" Version="193.20191030.267" />
    <PackageReference Include="JetBrains.Unity.Libs.2017.3.0f3" Version="2018.3.5" />
    <PackageReference Include="ILRepack.Lib.MSBuild.Task" Version="2.0.18" />
  </ItemGroup>
  <ItemGroup>
    <ILRepackInputAssemblies Include="../lib/*.dll">
      <Visible>false</Visible>
    </ILRepackInputAssemblies>
  </ItemGroup>
  <ItemGroup>
    <Reference Include="AsyncBridge.Net35, Version=0.0.0.0, Culture=neutral, PublicKeyToken=b3b1c0202c0d6a87">
      <HintPath>..\lib\AsyncBridge.Net35.dll</HintPath>
    </Reference>
    <Reference Include="JetBrains.Lifetimes">
      <HintPath>..\lib\JetBrains.Lifetimes.dll</HintPath>
    </Reference>
    <Reference Include="JetBrains.RdFramework">
      <HintPath>..\lib\JetBrains.RdFramework.dll</HintPath>
    </Reference>
    <Reference Include="System.Threading, Version=1.0.2856.102, Culture=neutral, PublicKeyToken=31bf3856ad364e35">
      <HintPath>..\lib\System.Threading.dll</HintPath>
    </Reference>
    <ILRepackInputAssemblies Include="$(OutputPath)JetBrains.Lifetimes.dll" />
    <ILRepackInputAssemblies Include="$(OutputPath)JetBrains.Lifetimes.dll" />
    <ILRepackInputAssemblies Include="$(OutputPath)JetBrains.RdFramework.dll" />
    <ILRepackInputAssemblies Include="$(OutputPath)AsyncBridge.Net35.dll" />
    <ILRepackInputAssemblies Include="$(OutputPath)System.Threading.dll" />
    <ILRepackInputAssemblies Include="../lib/JetBrains.Annotations.dll" />
  </ItemGroup>
  
  <ItemGroup>
    <EmbeddedResource Include="..\lib\pdb2mdb.exe" />
  </ItemGroup>
  
</Project>
